(def input "
           
           
           
           ")

; we just need to change the pad to make it work for part 1
(def pad
  {[0 2] \5
   [1 1] \2
   [1 2] \6
   [1 3] \A
   [2 0] \1
   [2 1] \3
   [2 2] \7
   [2 3] \B
   [2 4] \D
   [3 1] \4
   [3 2] \8
   [3 3] \C
   [4 2] \9
   })

(def moves
  {
   \U [0 -1]
   \D [0 1]
   \L [-1 0]
   \R [1 0]})

   
(defn next-val
  [[x y :as coords] input]
  (let [move (moves input)
        [new-x new-y :as new-coords] (map + coords move)
        val (pad new-coords)]
    (if (nil? val)
      coords
      new-coords
      )))

(defn reduce-line
  [line]
  (reduce 
    (fn [acc val]
      (next-val acc val))
    [0 2]
    line))

(def result 
  (map
    (comp #(pad %) reduce-line)
    (.split input "\n")))


(println result)
